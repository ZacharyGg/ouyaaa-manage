<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.ouyaaa.manage.equipment.mapper.MaintenanceResultMapper">

    <!-- 开启二级缓存 -->
    <cache type="org.mybatis.caches.ehcache.LoggingEhcache"/>

    <!-- 通用查询映射结果 -->
    <resultMap id="BaseResultMap" type="com.ouyaaa.manage.equipment.entity.MaintenanceResult">
        <id column="result_id" property="resultId" />
        <result column="result_code" property="resultCode" />
        <result column="result_name" property="resultName" />
        <result column="maint_id" property="maintId" />
        <result column="item_code" property="itemCode" />
        <result column="is_qualified" property="isQualified" />
        <result column="status_code" property="statusCode" />
        <result column="update_by" property="updateBy" />
        <result column="update_time" property="updateTime" />
        <result column="create_time" property="createTime" />
    </resultMap>

    <!-- 通用查询结果列 -->
    <sql id="Base_Column_List">
        result_id, result_code, result_name, maint_id, item_code, is_qualified, status_code, update_by, update_time, create_time
    </sql>

</mapper>
