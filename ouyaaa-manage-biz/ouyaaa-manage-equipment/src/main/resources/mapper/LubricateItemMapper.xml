        <result column="z_axis" property="zAxis" />
        <result column="d_code" property="dCode" />
        <result column="status_code" property="statusCode" />
        <result column="update_by" property="updateBy" />
        <result column="update_time" property="updateTime" />
    </resultMap>

    <!-- 通用查询结果列 -->
    <sql id="Base_Column_List">
        item_id, item_code, item_name, item_part, item_content, item_standart, item_cycle, cycle_unit, regulation_code, x_axis, y_axis, z_axis, d_code, status_code, update_by, update_time
