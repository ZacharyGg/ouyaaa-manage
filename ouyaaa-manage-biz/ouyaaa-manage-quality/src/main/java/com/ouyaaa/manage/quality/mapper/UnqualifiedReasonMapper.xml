<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.ouyaaa.manage.quality.mapper.UnqualifiedReasonMapper">

    <!-- 开启二级缓存 -->
    <cache type="org.mybatis.caches.ehcache.LoggingEhcache"/>

    <!-- 通用查询映射结果 -->
    <resultMap id="BaseResultMap" type="com.ouyaaa.manage.quality.entity.UnqualifiedReason">
        <id column="unqua_id" property="unquaId" />
        <result column="unqua_code" property="unquaCode" />
        <result column="unqua_name" property="unquaName" />
        <result column="item_id" property="itemId" />
        <result column="resolvent_code" property="resolventCode" />
        <result column="experience_code" property="experienceCode" />
        <result column="status_code" property="statusCode" />
        <result column="update_by" property="updateBy" />
        <result column="update_time" property="updateTime" />
    </resultMap>

    <!-- 通用查询结果列 -->
    <sql id="Base_Column_List">
        unqua_id, unqua_code, unqua_name, item_id, resolvent_code, experience_code, status_code, update_by, update_time
    </sql>

</mapper>
