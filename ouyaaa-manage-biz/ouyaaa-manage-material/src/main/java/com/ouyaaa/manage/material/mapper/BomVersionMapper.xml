<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.ouyaaa.manage.material.mapper.BomVersionMapper">

    <!-- 开启二级缓存 -->
    <cache type="org.mybatis.caches.ehcache.LoggingEhcache"/>

    <!-- 通用查询映射结果 -->
    <resultMap id="BaseResultMap" type="com.ouyaaa.manage.material.entity.BomVersion">
        <id column="version_id" property="versionId" />
        <result column="version_code" property="versionCode" />
        <result column="brand" property="brand" />
        <result column="start_time" property="startTime" />
        <result column="end_time" property="endTime" />
        <result column="version_status" property="versionStatus" />
        <result column="update_by" property="updateBy" />
        <result column="update_time" property="updateTime" />
    </resultMap>

    <!-- 通用查询结果列 -->
    <sql id="Base_Column_List">
        version_id, version_code, brand, start_time, end_time, version_status, update_by, update_time
    </sql>

</mapper>
